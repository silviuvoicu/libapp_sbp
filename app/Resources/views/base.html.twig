<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        <div id="top-menu" >
      {% if app.user %}
       {{ top_menu() }} 
       {% else %}
      <a href="{{ path('register') }}" >{{ top_menu() }}  </a>
      {% endif %}
               
        </div>
        
        {% block body %}{% endblock %}
        {% block javascripts %}{% endblock %}
            
                 
    </body>
</html>
